{
    "extends": "react-app",
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "error",
        "class-methods-use-this": 0,
        "import/no-named-as-default": 0,
        "react/jsx-filename-extension": [
            "error",
            {
                "extensions": [".js", ".jsx"]
            }
        ]
    }
}